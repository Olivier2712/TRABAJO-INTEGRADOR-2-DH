<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
 
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link width="" rel="shortcut icon" href="./images/logo-mercado-liebre.jpg" />
 
    <title> Mercado Liebre Argentina</title>
    <!-- Font Awesome Stylesheet -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
    <!-- Bootstrap Grid Stylesheet -->
    <link rel="stylesheet" href="/css/bootstrap-grid.min.css">
    <!-- Own Stylesheet -->
    <link rel="stylesheet" href="/css/app.css">
</head>
 
<body>
    <!-- Header -->
    <% if(logueado) { %>
        <%- include('../views/partials/headerLogueado.ejs') %>
    <% } else { %>
        <%- include('../views/partials/header.ejs') %>
    <% } %>
    <!-- /Header -->
 
    <!-- Novedades -->
    <div class="container products-wrapper">
        <div class="row">
            <div class="col-12">
                <h2 class="products-title">Agregar productos</h2>
            </div>
            <!-- Show the "ultimos" products -->
            <h2 class="col-12">
            </h2>
            <form  action="" method="get">
                <ul class="FormularioCarga">
                    <li><input class="form-input" type="file" name="imagenProducto" id="" placeholder="Imagen de producto"></li>
                    <li><input class="form-input" type="text" name="NombreProducto" id="" placeholder="Nombre del producto"></li>
                    <li><input class="form-input" type="text" name="DescripcionProducto" id="" placeholder="Descripcion del producto" ></li>
                    <li><input class="form-input" type="datetime" name="FechaCargaProducto" id="" placeholder="Fecha de carga del producto" ></li>
                    <li><button class="form-label" type="submit">Agregar Producto</button></li>
                </ul>
               
            </form>
 
 
    <!-- footer -->
    <%- include('../views/partials/footer.ejs')%>
    <!-- /footer -->
</body>
 
</html>